    "e2e:open": "cypress open",
    "e2e:server": "npm run e2e:migrate && NODE_TEST=1 npm run dev",
    "e2e:headless": "NODE_TEST=1 npm run migrate && NODE_TEST=1 npm run build && NODE_TEST=1 npm run start",
